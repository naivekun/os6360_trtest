#include "armada-3900-db.dtsi"

/ {
	model = "Marvell Armada 3900 Validation Development board";
	compatible = "marvell,armada3900-db-vd", "marvell,armada7040",
		     "marvell,armada-ap806-quad", "marvell,armada-ap806";

	cp0 {
		config-space {
			serial@702000 {
				status = "okay";
			};
			serial@702100 {
				status = "okay";
			};
			pinctrl@440000 {
				cp0_xhci0_vbus_pins: cp0-xhci0-vbus-pins {
					marvell,pins = "mpp44";
					marvell,function = "gpio";
				};
				cp0_xhci1_vbus_pins: cp0-xhci1-vbus-pins {
					marvell,pins = "mpp45";
					marvell,function = "gpio";
				};
			};
			cp0_usb3h0: usb3@500000 {
				status = "okay";
			};
			cp0_usb3h1: usb3@510000 {
				separated-phys-for-usb2-usb3;
				phys = <&cp0_comphy 4 COMPHY_USB3H1>;
				phy-names = "usb3";
				status = "okay";
			};
			mdio@12a600 {
				phy1: ethernet-phy@1 {
					max-speed = <2500>;
				};
			};
			gop {
				cp0_emac2: mac2 {
					phy-mode = "sgmii";
					phy-speed = <1000>;
					phys = <&cp0_comphy 3 COMPHY_SGMII1>;
				};
			};
		};

		pcie0@600000 {
			num-lanes = <2>;
			phys = <&cp0_comphy 0 COMPHY_PCIE0
				&cp0_comphy 1 COMPHY_PCIE0
				>;
			phy-names = "pcie-phy0", "pcie-phy1";

		};
	};

	cp0_reg_usb3_vbus0: cp0_usb3_vbus@0 {
		gpio = <&cp0_gpio1 12 GPIO_ACTIVE_HIGH>;
	};
	cp0_reg_usb3_vbus1: cp0_usb3_vbus@1 {
		gpio = <&cp0_gpio1 13 GPIO_ACTIVE_HIGH>;
	};
};
