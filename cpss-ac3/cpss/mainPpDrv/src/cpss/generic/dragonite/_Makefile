# $(warning "Started") 

LIB_NAME = cpssGenDragonite.lib

#List of files to include in list
FILES := \
        cpssGenDragonite.c \
        prvCpssGenDragoniteIntr.c

ifeq (L_ON,$(LOG_ENABLE))
FILES += \
        prvCpssGenDragoniteLog.c \
        prvCpssGenDragoniteManualLog.c 
endif

ifneq (A1, $(DRAGONITE_TYPE))
        #C_EXCLUDE_FILE_LIST = ALL
        FILES :=
else
        ifneq (L_ON,$(LOG_ENABLE))
                #C_EXCLUDE_FILE_LIST += prvCpssGenDragoniteLog.c prvCpssGenDragoniteManualLog.c
                FILES := \
                        cpssGenDragonite.c \
                        prvCpssGenDragoniteIntr.c
        endif
endif

#List of subfolders to include in build
MODULES := 




CWD := $(dir $(lastword $(MAKEFILE_LIST)))

#Add relative path to original Makefile, to each file in the list
MAINPPDRV_SRC += $(patsubst %,$(CWD)%,$(FILES))

#Enable this line to add include path of this folder
#CFLAGS := $(CFLAGS) -I$(CWD)

#Recursively go into list of subfolders
include $(patsubst %,$(CWD)%/$(MAKEFILE),$(MODULES))

