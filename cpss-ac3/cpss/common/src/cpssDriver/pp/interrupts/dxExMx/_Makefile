# $(warning "Started")
LIB_NAME = cpssDrvPpInterruptsDxExMx.lib


#List of files to include in list



FILES := \
 cpssDrvPpIntCtrl.c \
 cpssDrvPpIntInit.c \
 cpssDrvPpIntInterrupts.c


ifeq (EXISTS, $(CHX_FAMILY))
