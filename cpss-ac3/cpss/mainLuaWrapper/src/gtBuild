ifeq (yes,$(CMD_LUA_CLI))
  SUBDIRS      =

  ifeq (EXISTS, $(CHX_FAMILY))
      SUBDIRS      += dxCh
  endif
  ifeq (EXISTS, $(PM_FAMILY))
      SUBDIRS      += exMxPm
  endif
  ifeq (EXISTS, $(PX_FAMILY))
      MODULES += px
  endif

  ifeq (EXISTS, $(ASIC_SIMULATION))
      SUBDIRS      += simulation
  endif

  SUBDIRS      += cpssCommon generic
  ifeq (EXISTS, $(IMPL_TGF))
      SUBDIRS += tgf
  endif

else
  SUBDIRS = NONE
endif


