	DX_FILES += gtAppDemoTmConfig.c 
	DX_FILES += gtAppDemoTmUtils.c  
endif


PM_FILES := gtAppDemoExMxPmSysConfig.c

FILES := gtAppInit.c gtAppDemoGenSysConfig.c gtAppDemoPciConfig.c gtAppDemoSmiConfig.c appDemoDb.c
FILES += appDemoStaticDb.c gtAppDemoZarlinkConfig.c



XBAR_FILES = gtAppDemoXbarConfig_stub.c
ifeq (PRESTERA, $(XBAR_VENDOR))
  XBAR_FILES := gtAppDemoXbarConfig.c
endif

FA_FILES = gtAppDemoFaConfig_stub.c
ifeq (PRESTERA, $(FA_VENDOR))
  FA_FILES := gtAppDemoFaConfig.c
endif


ifeq (EXIST, $(EXMX_FAMILY))
 FILES += $(EXMX_FILES)
 USE_FA_FILES = USED
 USE_XBAR_FILES = USED
endif

ifeq (EXIST, $(DX_FAMILY))
 FILES += $(DX_FILES)
endif

ifeq (EXIST, $(PM_DEVICE))
 FILES += $(PM_FILES)
endif

ifeq (USED, $(USE_FA_FILES))
 FILES += $(FA_FILES)
endif

ifeq (USED, $(USE_XBAR_FILES))
 FILES += $(XBAR_FILES)
endif

